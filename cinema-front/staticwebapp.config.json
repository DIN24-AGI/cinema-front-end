{
	"routes": [
		{
			"route": "/*",
			"serve": "/index.html",
			"statusCode": 200
		}
	],
	"navigationFallback": {
		"rewrite": "/index.html",
		"exclude": ["/img/*", "/css/*", "/js/*", "*.svg", "*.png", "*.jpg", "*.jpeg", "*.gif", "*.webp"]
	},
	"mimeTypes": {
		".webmanifest": "application/manifest+json"
	}
}
